{
  "name": "norcams-named_interfaces",
  "version": "0.2.0",
  "author": "norcams",
  "summary": "Set up fact aliases for network interfaces",
  "license": "Apache-2.0",
  "source": "https://github.com/norcams/puppet-named_interfaces",
  "project_page": "https://github.com/norcams/puppet-named_interfaces",
  "issues_url": "https://github.com/norcams/puppet-named_interfaces/issues",
  "operatingsystem_support": [
    {
